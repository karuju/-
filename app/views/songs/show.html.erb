<div class="col-lg-10 offset-lg-1">
  <h1>Song#show</h1>
  <p>Find me in app/views/song/show.html.erb</p>
  <div>
  <%= render "song", song: @song %>
    <%= @song.release_date %>
  </div>
    <div class="link_button">
      <%= link_to "一覧", songs_path %>
    </div>
    <div class="link_button">
      <%= link_to "編集", edit_song_path(@song) %>
    </div>
    <div class="link_button">
      <%= link_to song_path(@song), data: { turbo_method: :delete, turbo_confirm: t('削除しますか？') } do %>
        削除
      <% end %>
    </div>
</div>